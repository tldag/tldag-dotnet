<?xml version="1.0" encoding="utf-8" ?>
<Project>

  <PropertyGroup>
    <TldagTasksFW Condition="'$(MSBuildRuntimeType)' == 'Core'">net5.0</TldagTasksFW>
    <TldagTasksFW Condition="'$(MSBuildRuntimeType)' != 'Core'">net472</TldagTasksFW>
    <TldagTasksLib>$([MSBuild]::NormalizePath( $(MSBuildThisFileDirectory), '..', 'lib', $(TldagTasksFW), 'Tldag.Tasks.dll' ))</TldagTasksLib>
  </PropertyGroup>

  <UsingTask TaskName="TLDAG.Tasks.Hello" AssemblyFile="$(TldagTasksLib)" />
  
</Project>
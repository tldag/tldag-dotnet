<Project>

  <PropertyGroup>
    <PackageOutputPath>$([MSBuild]::NormalizeDirectory( $(MSBuildThisFileDirectory), 'packages' ))</PackageOutputPath>
    <ModuleOutputPath>$([MSBuild]::NormalizeDirectory( $(MSBuildThisFileDirectory), 'modules' ))</ModuleOutputPath>
  </PropertyGroup>

  <PropertyGroup>
    <VersionPrefix>0.0.3</VersionPrefix>
    <VersionSuffix></VersionSuffix>
  </PropertyGroup>
  
  <PropertyGroup>
    <LangVersion>9.0</LangVersion>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup>
    <TldagLibrariesCodeGen>$([MSBuild]::NormalizePath( $(MSBuildThisFileDirectory), 'TLDAG.Libraries.CodeGen', 'TLDAG.Libraries.CodeGen.csproj' ))</TldagLibrariesCodeGen>
    <TldagLibrariesCore>$([MSBuild]::NormalizePath( $(MSBuildThisFileDirectory), 'TLDAG.Libraries.Core', 'TLDAG.Libraries.Core.csproj' ))</TldagLibrariesCore>
  </PropertyGroup>
  
</Project>
